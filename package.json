{
  "name": "daynews",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "./dev.sh",
    "build": "vite build",
    "start": "node server/server-enhanced.js",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "dev:ssr": "nodemon server/index.js",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr src/entry/entry-server.tsx --outDir dist/server",
    "build:ssr": "npm run build:client && npm run build:server",
    "serve:ssr": "node server/server-enhanced.js",
    "build:prod": "./build-prod.sh",
    "preview": "vite preview",
    "quick-start": "./quick-start.sh",
    "test": "node tests/run-all-tests.js",
    "test:all": "node tests/run-all-tests.js --suites unit,integration,e2e,benchmarks",
    "test:unit": "node tests/run-all-tests.js --suites unit",
    "test:integration": "node tests/run-all-tests.js --suites integration",
    "test:e2e": "node tests/run-all-tests.js --suites e2e",
    "test:benchmarks": "node tests/run-all-tests.js --suites benchmarks",
    "test:coverage": "node tests/run-all-tests.js --coverage",
    "test:watch": "node tests/run-all-tests.js --suites unit --verbose",
    "test:fast": "node tests/run-all-tests.js --suites unit,integration --no-coverage",
    "test:ci": "node tests/run-all-tests.js --fail-fast --no-report",
    "test:verbose": "node tests/run-all-tests.js --verbose",
    "test:help": "node tests/run-all-tests.js --help",
    "test:legacy": "node tests/run-tests.js all",
    "test:legacy:unit": "node tests/run-tests.js unit",
    "test:legacy:integration": "node tests/run-tests.js integration",
    "test:report": "node tests/test-summary-generator.js",
    "test:report:html": "node tests/test-summary-generator.js --format html",
    "test:report:md": "node tests/test-summary-generator.js --format markdown",
    "test:report:all": "node tests/test-summary-generator.js --format all",
    "benchmark": "node tests/benchmarks/run-benchmarks.js",
    "benchmark:cache": "node tests/benchmarks/run-benchmarks.js --suites cache",
    "benchmark:rendering": "node tests/benchmarks/run-benchmarks.js --suites rendering",
    "benchmark:data": "node tests/benchmarks/run-benchmarks.js --suites dataLayer",
    "benchmark:ci": "node tests/benchmarks/run-benchmarks.js --alert --no-verbose",
    "benchmark:verbose": "node tests/benchmarks/run-benchmarks.js --verbose",
    "validate": "node scripts/validate-deployment.js",
    "validate:fix": "node scripts/fix-route-config-syntax.js && node scripts/fix-route-patterns.js",
    "test:frontend": "NODE_OPTIONS='--experimental-vm-modules' npx jest tests/frontend/",
    "prebuild": "node scripts/validate-deployment.js || true",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .vite cache",
    "clean:all": "npm run clean && rm -rf node_modules package-lock.json"
  },
  "dependencies": {
    "@babel/generator": "^7.28.3",
    "@babel/parser": "^7.28.3",
    "@babel/traverse": "^7.28.3",
    "@babel/types": "^7.28.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.55.0",
    "@tanstack/react-query": "^5.85.3",
    "@testing-library/react": "^16.3.0",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "lucide-react": "^0.539.0",
    "postcss": "^8.5.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.8.0",
    "tailwind-merge": "^3.3.1",
    "vite": "^7.1.2",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.12",
    "@testing-library/jest-dom": "^6.7.0",
    "@types/express": "^5.0.3",
    "@types/node": "^20.19.11",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "eslint": "^9",
    "glob": "^11.0.3",
    "jest": "^30.0.5",
    "msw": "^2.10.5",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4",
    "tailwindcss": "^4.1.12",
    "typescript": "^5.9.2"
  },
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
